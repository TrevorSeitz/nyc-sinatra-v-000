<label>Landmarks: </label>


<table>
  <tr>
    <th>Name</th>
    <th>Year Completed</th>
  </tr>
  <% Landmark.all.each do |landmark| %>
  <tr>
    <td><a href="/landmarks/<%= landmark.id %>"><%= landmark.name %></td>
    <td><%= landmark.year_completed %></td>
  </tr>
<table>
<br><br>
<form action="/figures/new" method="get">
  <input type="submit" id="forward_to_new_figure" value="New Figure">
</form>
<% end %>
